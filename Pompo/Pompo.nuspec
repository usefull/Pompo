<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>Pompo</id>
    <version>1.1.0</version>
    <authors>usefull</authors>
    <description>The brige to create and use .NET objects in JS code via WebAssembly.</description>
	<readme>docs\README.md</readme>
    <repository type="git" url="https://github.com/usefull/Pompo.git" />
	<projectUrl>https://github.com/usefull/Pompo</projectUrl>
    <dependencies>
	    <group targetFramework="net9.0" />
    </dependencies>
  </metadata>
	<files>
		<file src="bin\Release\net9.0\Pompo.dll" target="lib\net9.0\Pompo.dll" />
		<file src="bin\Release\net9.0\Pompo.MSBuild.dll" target="lib\net9.0\Pompo.MSBuild.dll" />
		<file src="bin\Release\net9.0\*.targets" target="build" />
		<file src="bin\Release\net9.0\*.props" target="build" />
		<file src="bin\Release\net9.0\Pompo.CodeGenerator.dll" target="analyzers\dotnet\cs\Pompo.CodeGenerator.dll" />
		<file src="..\README.md" target="docs\" />
	</files>
</package>
